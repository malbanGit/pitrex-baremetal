ROM:7D9A ; =============== S U B R O U T I N E =======================================
ROM:7D9A
ROM:7D9A
ROM:7D9A sub_7D9A:
ROM:7D9A                 ldd     <Stars_XT       ; Get X 'forward' transform position
ROM:7D9C                 std     Math_XT2        ; XT2
ROM:7D9F                 ldd     Stars_YT        ; Get Y 'horizontal' transform
ROM:7DA2                 std     Math_YT2        ; YT2
ROM:7DA5                 ldd     Stars_ZT        ; Get Z 'vertical' transform
ROM:7DA8                 std     Math_ZT2        ; ZT2
ROM:7DAB                 ldx     #Scratch_RAM_start
ROM:7DAE                 ldd     #0
ROM:7DB1
ROM:7DB1 loc_7DB1:
ROM:7DB1                 std     ,x++
ROM:7DB3                 cmpx    #Scratch_RAM_end ; Clear star dots RAM
ROM:7DB6                 bcs     loc_7DB1
ROM:7DB8                 sta     <DPbyte_88      ; Clear DPbyte_88
ROM:7DBA                 lda     #$67 ; 'g'      ; Insert Vector STAT 7 for white stars
ROM:7DBC                 ldb     <DPbyte_83      ; Star intensity
ROM:7DBE                 std     ,y++            ; Y points to vector RAM
ROM:7DC0                 ldd     #$18C           ; Point BIC to Math RAM $5C60
ROM:7DC3                 std     MW1
ROM:7DC6                 stb     <DPbyte_82      ; Store $8C to DPbyte_82
ROM:7DC8                 negb                    ; Negate to $74
ROM:7DC9                 addb    #$BE ; '+'      ; Add $BE to get $32
ROM:7DCB                 stb     <DPbyte_81      ; Store $32 in DPbyte_81 (why not just load B with $32 ?)
ROM:7DCD                 ldu     #Scratch_RAM_end
ROM:7DD0                 ldx     #Star_Dots_MRAM
ROM:7DD3
ROM:7DD3 loc_7DD3:                               ; DPbyte_84 holds running star MRAM pointer
ROM:7DD3                 stx     <DPbyte_84
ROM:7DD5                 lda     #$67 ; 'g'
ROM:7DD7                 jsr     Math_Run_Start
ROM:7DDA                 ldd     MReg00          ; Math result X
ROM:7DDD                 cmpd    #$100
ROM:7DE1                 lble    loc_7E5D
ROM:7DE5                 cmpd    #$FFF
ROM:7DE9                 lbhi    loc_7E5D        ; Check perspective division value?
ROM:7DED                 std     DVSRH           ; Start division
ROM:7DF0                 ldd     MReg39          ; Reg39 = Reg01 x Reg01
ROM:7DF3                 subd    MReg38          ; Reg38 = Reg00 x Reg00
ROM:7DF6                 lbcc    loc_7E5D
ROM:7DFA                 ldd     MReg3A          ; Reg3A = Reg02 x Reg02
ROM:7DFD                 subd    MReg38
ROM:7E00
ROM:7E00 loc_7E00:
ROM:7E00                 lbcc    loc_7E5D
ROM:7E04                 ldd     MW0             ; Get division result
ROM:7E07                 std     MReg00          ; Math result X
ROM:7E0A                 lda     #$86 ; 'å'      ; Reg01 = Reg01 x Reg00
ROM:7E0A                                         ; Reg02 = Reg02 x Reg00
ROM:7E0C                 jsr     Math_Run_Start
ROM:7E0F                 clrb
ROM:7E10                 lda     MReg02          ; Math result Z
ROM:7E13                 lsra
ROM:7E14                 rolb
ROM:7E15                 lsra
ROM:7E16                 rolb
ROM:7E17                 lda     MReg01          ; Math result Y
ROM:7E1A                 lsra
ROM:7E1B                 rolb
ROM:7E1C                 lsra
ROM:7E1D                 rolb
ROM:7E1E                 lda     #8
ROM:7E20                 mul
ROM:7E21                 ldx     #Scratch_RAM_start
ROM:7E24                 abx
ROM:7E25                 ldd     2,x
ROM:7E27                 bne     loc_7E2D
ROM:7E29                 stu     2,x
ROM:7E2B                 bra     loc_7E2F
ROM:7E2D ; ---------------------------------------------------------------------------
ROM:7E2D
ROM:7E2D loc_7E2D:
ROM:7E2D                 stu     [,x]
ROM:7E2F
ROM:7E2F loc_7E2F:
ROM:7E2F                 stu     ,x
ROM:7E31                 ldd     #0
ROM:7E34                 std     ,u
ROM:7E36                 ldd     MReg02          ; Math result Z
ROM:7E39                 subd    6,x
ROM:7E3B                 anda    #$1F
ROM:7E3D                 std     4,u
ROM:7E3F                 ldd     MReg01          ; Math result Y
ROM:7E42                 subd    4,x
ROM:7E44                 anda    #$1F
ROM:7E46                 cmpu    2,x
ROM:7E49                 beq     loc_7E4D        ; These 3 lines surely do nothing as ORA #0 does nothing ?
ROM:7E4B                 oraa    #0
ROM:7E4D
ROM:7E4D loc_7E4D:
ROM:7E4D                 std     2,u
ROM:7E4F                 ldd     MReg02          ; Math result Z
ROM:7E52                 std     6,x
ROM:7E54                 ldd     MReg01          ; Math result Y
ROM:7E57                 std     4,x
ROM:7E59                 leau    6,u
ROM:7E5B                 bra     loc_7E62
ROM:7E5D ; ---------------------------------------------------------------------------
ROM:7E5D
ROM:7E5D loc_7E5D:
ROM:7E5D                 ldx     <DPbyte_84
ROM:7E5F                 jsr     sub_7F9A
ROM:7E62
ROM:7E62 loc_7E62:
ROM:7E62                 inc     <DPbyte_82
ROM:7E64                 ldx     <DPbyte_84
ROM:7E66                 leax    8,x
ROM:7E68                 dec     <DPbyte_81
ROM:7E6A                 lbne    loc_7DD3
ROM:7E6E                 ldx     #Scratch_RAM_start
ROM:7E71
ROM:7E71 loc_7E71:
ROM:7E71                 ldu     2,x
ROM:7E73                 beq     loc_7E95
ROM:7E75                 ldd     #$1F98          ; Starfield Y offset
ROM:7E78                 std     ,y++
ROM:7E7A                 ldd     #0
ROM:7E7D                 std     ,y++
ROM:7E7F
ROM:7E7F loc_7E7F:
ROM:7E7F                 ldd     4,u
ROM:7E81                 std     ,y++
ROM:7E83                 ldd     2,u
ROM:7E85                 std     ,y++
ROM:7E87                 ldd     word_33DC       ; JSRL to Vector ROM subroutine to draw dot
ROM:7E8A                 std     ,y++
ROM:7E8C                 ldu     ,u
ROM:7E8E                 bne     loc_7E7F
ROM:7E90                 ldd     #$8040          ; Insert Vector CNTR
ROM:7E93                 std     ,y++
ROM:7E95
ROM:7E95 loc_7E95:
ROM:7E95                 leax    8,x
ROM:7E97                 cmpx    #Scratch_RAM_end
ROM:7E9A                 bcs     loc_7E71
ROM:7E9C                 ldd     MReg4C
ROM:7E9F                 std     Math_XT2        ; XT2
ROM:7EA2                 ldd     MReg4D
ROM:7EA5                 std     Math_YT2        ; YT2
ROM:7EA8                 ldd     MReg4E
ROM:7EAB                 std     Math_ZT2        ; ZT2
ROM:7EAE                 rts
