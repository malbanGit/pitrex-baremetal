ROM:7FB8                 bmi     loc_7FBB
ROM:7FBA                 nega
ROM:7FBB
ROM:7FBB loc_7FBB:
ROM:7FBB                 std     4,x
ROM:7FBD                 lda     <DPbyte_82
ROM:7FBF                 sta     MW2
ROM:7FC2                 lda     #$60 ; '`'
ROM:7FC4                 jsr     Math_Run_Start
ROM:7FC7                 ldd     MReg00          ; Math result X
ROM:7FCA                 std     ,x
ROM:7FCC                 ldd     MReg01          ; Math result Y
ROM:7FCF                 std     2,x
ROM:7FD1                 ldd     MReg02          ; Math result Z
ROM:7FD4                 anda    #$FF            ; These 2 lines do nothing ?
ROM:7FD6                 andb    #$FF
ROM:7FD8                 std     4,x
ROM:7FDA                 rts
