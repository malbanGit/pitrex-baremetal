ROM:7FE2                 oraa    #$70 ; 'p'
ROM:7FE4                 ldb     <DPbyte_54
ROM:7FE6                 addd    Math_XT2        ; XT2
ROM:7FE9                 std     ,x
ROM:7FEB                 lda     <DPbyte_55
ROM:7FED                 anda    #$7F ; ''
ROM:7FEF                 tst     MReg01          ; Math result Y
ROM:7FF2                 bmi     loc_7FF5
ROM:7FF4                 nega
